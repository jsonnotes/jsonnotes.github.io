{
  "name": "jsonview-workspace",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "core",
    "lib",
    "client"
  ],
  "scripts": {
    "dev": "npm run dev -w @jsonview/client",
    "build": "npm run build -w @jsonview/client",
    "build:gh-pages": "npm run build -w @jsonview/core && npm run build:gh-pages -w @jsonview/client",
    "build:local": "npm run build -w @jsonview/core && npm run build:local -w @jsonview/client",
    "preview": "npm run preview -w @jsonview/client",
    "test": "npm run test -w @jsonview/lib",
    "check": "npm run check -w @jsonview/core && npm run check -w @jsonview/client",
    "db": "node scripts/db-cli.js",
    "db:add-note": "npm run db add-note",
    "db:get-note": "npm run db get-note",
    "db:remote": "npm run db remote",
    "db:sql": "npm run db sql",
    "db:run-local": "npm run db run-local",
    "db:pull": "npm run db pull",
    "db:push": "SPACETIMEDB_PRESET=prod npm run db push",
    "db:new": "npm run db new"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  }
}
