{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "BUILD_TARGET=gh-pages vite build --force && cp docs/index.html docs/404.html",
    "build:gh-pages": "BUILD_TARGET=gh-pages vite build --force && cp docs/index.html docs/404.html",
    "build:local": "BUILD_TARGET=local vite build --force",
    "preview": "vite preview",
    "test": "node scripts/test.js",
    "check": "tsc --noEmit",
    "build:tsc": "tsc -p tsconfig.build.json",
    "db": "node scripts/db-cli.js",
    "db:add-note": "npm run db add-note",
    "db:get-note": "npm run db get-note",
    "db:remote": "npm run db remote",
    "db:sql": "npm run db sql",
    "db:run-local": "npm run db run-local",
    "db:pull": "npm run db pull",
    "db:push": "SPACETIMEDB_PRESET=prod npm run db push",
    "db:new": "npm run db new"
  },
  "devDependencies": {
    "@clockworklabs/spacetimedb-sdk": "^1.1.0",
    "ajv": "^8.17.1",
    "typescript": "^4.5.4",
    "vite": "^3.0.7"
  }
}
